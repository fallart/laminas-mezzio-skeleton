{
    "name": "afaller/psr7-routes-builder",
    "description": "Library provides auto routing for every psr-7 & psr-15 application based on folder structure",
    "require": {
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2.3.0",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Artur Faller",
            "email": "3fallart3@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AFaller\\Psr7RoutesBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AFaller\\Psr7RoutesBuilderTest\\": "test/",
            "AFaller\\Psr7RoutesBuilderExample\\": "example/"
        }
    }
}
